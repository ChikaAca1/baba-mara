        {
          "type": "host",
          "value": "mysticcup.baba-mara.com"
        }
      ],
      "destination": "/en",
      "permanent": true
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "fal.baba-mara.com"
        }
